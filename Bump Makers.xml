	<Macro index="57" name="Bump Executer Maker">
		<Macroline index="0">
			<text>SetVar $bumpgroup=(&quot;Group?&quot;)</text>
		</Macroline>
		<Macroline index="1">
			<text>SetVar $bumpdests=(&quot;Empty Group Pool&quot;)</text>
		</Macroline>
		<Macroline index="2">
			<text>SetVar $bumpexec=(&quot;Where to Store?&quot;)</text>
		</Macroline>
		<Macroline index="3">
			<text>Group $bumpgroup</text>
		</Macroline>
		<Macroline index="4">
			<text>MAtricksFilter &quot;OddID&quot;</text>
		</Macroline>
		<Macroline index="5">
			<text>Store Group $bumpdests</text>
		</Macroline>
		<Macroline index="6">
			<text>Label Group $bumpdests &quot;Odd Bump Parts&quot;</text>
		</Macroline>
		<Macroline index="7">
			<text>Select Executor $bumpexec</text>
		</Macroline>
		<Macroline index="8">
			<text>Group $bumpdests At Full</text>
		</Macroline>
		<Macroline index="9">
			<text>At Preset 4.30</text>
		</Macroline>
		<Macroline index="10">
			<text>Store Cue 1 /cueonly=true</text>
		</Macroline>
		<Macroline index="11">
			<text>AddVar $bumpdests = 1</text>
		</Macroline>
		<Macroline index="12">
			<text>ClearAll</text>
		</Macroline>
		<Macroline index="13">
			<text>Group $bumpgroup</text>
		</Macroline>
		<Macroline index="14">
			<text>MAtricksFilter &quot;EvenID&quot;</text>
		</Macroline>
		<Macroline index="15">
			<text>Store Group $bumpdests</text>
		</Macroline>
		<Macroline index="16">
			<text>Label Group $bumpdests &quot;Even Bump Parts&quot;</text>
		</Macroline>
		<Macroline index="17">
			<text>Group $bumpdests At Full</text>
		</Macroline>
		<Macroline index="18">
			<text>At Preset 4.30</text>
		</Macroline>
		<Macroline index="19">
			<text>Store Cue 2</text>
		</Macroline>
		<Macroline index="20">
			<text>ClearAll</text>
		</Macroline>
		<Macroline index="21">
			<text>Group $bumpgroup At 0</text>
		</Macroline>
		<Macroline index="22">
			<text>At Preset 4.31</text>
		</Macroline>
		<Macroline index="23">
			<text>Store Cue 1.5</text>
		</Macroline>
		<Macroline index="24">
			<text>Group $bumpgroup At 0</text>
		</Macroline>
		<Macroline index="25">
			<text>At Preset 4.31</text>
		</Macroline>
		<Macroline index="26">
			<text>Store Cue 2.5</text>
		</Macroline>
		<Macroline index="27">
			<text>Assign Cue 1.5 + 2.5 /trig=follow /fade=0.5</text>
		</Macroline>
		<Macroline index="28">
			<text>AddVar $bumpgroup = &quot; Bump&quot;</text>
		</Macroline>
		<Macroline index="29">
			<text>Label Executor $bumpexec $bumpgroup</text>
		</Macroline>
		<Macroline index="30">
			<text>ClearAll</text>
		</Macroline>
	</Macro>
